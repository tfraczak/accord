{
  "compilerOptions": {
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "module": "ES2020",
    "allowJs": true,
    "checkJs": true,
    "target": "es6",
    "jsx": "react",
    "outDir": "./app/assets/builds",
    "paths": {
      "@axios": ["./app/client/helpers/axios"],
      "@constants": ["./app/client/constants"],
      "@helpers": ["./app/client/helpers"],
      "@types": ["./app/client/types"],
      "@utils": ["./app/client/utils"],
      "@selectors": ["./app/client/selectors"]
    },
    "types": ["node"],
    "typeRoots": ["app/client/types/global.d.ts"]
  }
}